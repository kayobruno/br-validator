{
    "name": "kayobruno/br-validator",
    "description": "Biblioteca PHP para validação de documentos brasileiros (CNPJ, CPF, CNS e etc.)",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "BrValidator\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "BrValidator\\Tests\\": "tests/"
      }
    },
    "require": {
      "php": "^8.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
      "test": "vendor/bin/phpunit",
      "lint": "php-cs-fixer fix --dry-run --diff",
      "fix": "php-cs-fixer fix",
      "analyse": "phpstan analyse"
    }
  }
